        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var bp_code;

if ( (max_bp_systolic &gt; 140) || (max_bp_diastolic &gt; 90) ) {
	bp_code = 3; &#47;&#47; &apos;high&apos;
} else if ( (max_bp_systolic &gt; 130) || (max_bp_diastolic &gt; 80) ) {
	&#47;&#47; No need to test upper bounds, since higher values will be caught by 
	&#47;&#47; the statement above.
	bp_code = 2; &#47;&#47; &apos;borderline&apos;
} else if ( (max_bp_systolic &lt;= 130) &amp;&amp; (max_bp_diastolic &lt;= 80) ) {
	bp_code = 1; &#47;&#47; &apos;normal&apos;
} else {
	bp_code = 4; &#47;&#47; &apos;not checked&apos;
};
</jsScript_script>
